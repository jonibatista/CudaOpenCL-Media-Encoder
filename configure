#!/bin/bash

DOC_IMAGE="cuda.jpg"

echo -e "\nConfiguring ...";

src="documentation/"
    
for dir in `ls "$src/"`
do
  if [ -d "$src/$dir" ]; then
    rm -r $src/$dir;
  fi
done

echo -e "Updating tree.out ...";
# Create tree output
tree -o documentation/tree.out #>/dev/null 2>&1

echo -e "Generating documentation ...";
cd _Codificador/
make docs >/dev/null 2>&1


echo -e "\nDone.\n";
